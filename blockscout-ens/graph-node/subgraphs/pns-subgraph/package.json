{
  "name": "pns-subgraph",
  "version": "1.0.2",
  "repository": "https://github.com/pulsedomains/pns-subgraph",
  "license": "MIT",
  "scripts": {
    "codegen": "graph codegen --output-dir src/types/",
    "test": "graph test",
    "setup": "yarn codegen && yarn create-local && yarn deploy-local",
    "create-local": "graph create graphprotocol/pns --node http://127.0.0.1:8020",
    "deploy-local": "graph deploy graphprotocol/pns --ipfs http://localhost:5001 --node http://127.0.0.1:8020/ --version-label 0.0.1",
    "remove-local": "graph remove graphprotocol/pns --node http://127.0.0.1:8020",
    "docker:setup": "yarn codegen && yarn docker:create-local && yarn docker:deploy-local",
    "docker:create-local": "graph create graphprotocol/pns --node https://pns-app_graph-node_1:8020",
    "docker:deploy-local": "graph deploy graphprotocol/pns --ipfs https://pns-app_ipfs_1:5001 --node http://pns-app_graph-node_1:8020/ --version-label 0.0.1",
    "docker:remove-local": "graph remove graphprotocol/pns --node https://pns-app_graph-node_1:8020",
    "build": "graph build",
    "deploy": "graph deploy --network mainnet --product hosted-service pnsdomains/pns",
    "watch-local": "graph deploy graphprotocol/pns --watch --debug --node http://127.0.0.1:8020/ --ipfs http://localhost:5001"
  },
  "devDependencies": {
    "@ensdomains/content-hash": "^2.5.3",
    "@graphprotocol/graph-cli": "^0.51.2",
    "@graphprotocol/graph-ts": "^0.31.0",
    "assemblyscript": "^0.19.0",
    "matchstick-as": "^0.5.2",
    "typescript": "^4.9.4"
  }
}
