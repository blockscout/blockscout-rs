{
  "name": "@blockscout/interchain-indexer-types",
  "version": "0.0.1",
  "description": "TypeScript definitions for Interchain Indexer microservice",
  "main": "./index.js",
  "types": "./index.d.ts",
  "scripts": {
    "build": "npm run compile:proto && npm run compile:ts",
    "compile:proto": "mkdir -p ./dist && protoc --plugin=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_opt=snakeToCamel=false --ts_proto_opt=exportCommonSymbols=false --ts_proto_opt=stringEnums=true --ts_proto_opt=onlyTypes=true --ts_proto_opt=emitImportedFiles=false --proto_path=../ --proto_path=../../proto/ --ts_proto_out=./dist ../interchain-indexer-proto/proto/v1/interchain_indexer.proto ../interchain-indexer-proto/proto/v1/stats.proto ../interchain-indexer-proto/proto/v1/status.proto",
    "compile:ts": "tsc --declaration ./index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blockscout/blockscout-rs.git",
    "directory": "interchain-indexer/types"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/blockscout/blockscout-rs/issues"
  },
  "homepage": "https://github.com/blockscout/blockscout-rs#readme",
  "devDependencies": {
    "ts-proto": "1.176.2",
    "typescript": "5.4.5"
  }
}
