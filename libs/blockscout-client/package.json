    "lodash": "^4.17.21",
    "node-fetch": "2",
    "yaml": "^2.8.0",
    "yanl": "^0.0.0"
  },
  "scripts": {
    "getV2": "ts-node ts-src/get-api-v2.ts",
    "merge": "ts-node ts-src/merge-overrides.ts",
    "generate": "docker run --rm -v \"${PWD}:/local\" openapitools/openapi-generator-cli generate -c /local/openapi-generator-config.json -i /local/swaggers/blockscout-api-final.yaml --skip-validate-spec -g rust -o /local/crate --engine handlebars -t /local/rust-template-handlebars --type-mappings=integer=i64",
    "format": "cargo fmt --all -- --config imports_granularity=Crate",
    "generate-from-remote": "yarn getV2 && yarn merge && yarn generate && yarn format",
    "record-stubs": "ts-node ts-src/record-stubs.ts"
  },
